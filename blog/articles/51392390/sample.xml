<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="Google Toolbar ガジェットサンプル"
               height="100"
               scrolling="true">
  </ModulePrefs>
  <UserPref name="page_url" display_name="URL" />
  <UserPref name="page_host" display_name="HOST" />
  <UserPref name="rawquery" display_name="検索文字列" />
  <UserPref name="selectedtext" display_name="選択文字列" />
  <UserPref name="option1" display_name="Option1" />

  <Content type="html"><![CDATA[

<div style="font-size: 12px;">
URL : __UP_page_url__<br>
HOST : __UP_page_host__<br>
検索文字列 : __UP_rawquery__<br>
選択文字列 : __UP_selectedtext__<br>
option1 : __UP_option1__<br>
</div>

  ]]></Content>

</Module>
